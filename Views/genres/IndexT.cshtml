@model List<hypster_tv_DAL.Mix_GetGenresAndCovers>
@{
    ViewBag.Title = "Hypster - free music online";
    ViewBag.ActivePage = "genres";
    Layout = "~/Views/Shared/_LayoutT.cshtml";
}




<script type="text/javascript">
    active_menu = ".mCreate";
    $(".mCreate").css("background-color", "#41606e");
</script>





<!-- background-color:#313537; -->
<div style=" float:left; width:100%; min-height:10px; overflow:hidden; margin-top:0px; ">
    
    @foreach (var item in Model)
    {
        if (@item.Mix_Cover != null)
        {
            <a href="/listen/@item.GenreName.ToLower().Replace(' ', '_')" style=" float:left; width:95px; margin:3px; background-color:#353535; padding:2px; border-radius:2px;">
                <img alt="" src="@item.Mix_Cover" width="90" style=" margin-top:2px;" />
                <span style=" display:block; float:left; width:100%; height:18px; line-height:18px; overflow:hidden;">
                    @item.GenreName
                </span>
            </a>
        }
    }
</div>





