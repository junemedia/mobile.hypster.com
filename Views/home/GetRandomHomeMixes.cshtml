@model List<hypster_tv_DAL.Mix_Mobile>
@{
    Layout = null;
}



<div style=" float:left; width:100%; text-align:left; font-size:16px; color:#bbbbbb; margin-bottom:5px;">
    &nbsp;&nbsp;Popular playlists                
</div>


<div style=" float:left; width:99%; margin-left:1%; ">
@foreach (var item in Model)
{
    <div style=" float:left; width:95px; margin:3px; background-color:#353535; padding:2px; border-radius:2px;">
        <img alt="" src="@item.Mix_Cover" width="90" style=" margin-top:2px;" />
        <div style=" float:left; width:100%; height:18px; line-height:18px; overflow:hidden;">
            @item.Mix_Name
        </div>
    </div>
}
</div>

